    "globals": "^15.0.0",
    "mdsvex": "^0.11.2",
    "postcss": "^8.4.49",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "storybook": "^8.4.5",
    "svelte": "^5.2.11",
